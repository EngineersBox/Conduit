{
    "connection": {
        "type": "JMX",
        "cache_key": "localhost_jmx_server",
        "config": {
            "url": "service:jmx:rmi://localhost/jndi/rmi://localhost:5555/jmxrmi"
        }
    },
    "configuration": {
        "json_provider": "JACKSON",
        "mapping_provider": "JACKSON"
    },
    "event_template": {
        "host": "Test Service",
        "description": "Some Metric Description",
        "metric_d": 123.44
    },
    "metrics": [
        {
            "namespace": "/jmx/example/count",
            "path": "$.jmxMBean(\"SimpleAgent:name=test\",\"count\")",
            "structure": {
                "type": "INTEGER"
            }
        },
        {
            "namespace": "/jmx/example/value",
            "path": "$.jmxMBean(\"SimpleAgent:name=test\",\"value\")",
            "structure": {
                "type": "DOUBLE"
            }
        }
    ]
}